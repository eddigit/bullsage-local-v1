# Frontend Environment Variables
# Copy this file to .env and update the values

# URL du backend API (sans slash final)
REACT_APP_BACKEND_URL=http://localhost:8000

# Pour le déploiement en production, utilisez l'URL de votre API déployée :
# REACT_APP_BACKEND_URL=https://bullsage-api.onrender.com

# Configuration WebSocket (pour le développement local)
WDS_SOCKET_PORT=3000

# Fonctionnalités optionnelles
REACT_APP_ENABLE_VISUAL_EDITS=false
ENABLE_HEALTH_CHECK=false
